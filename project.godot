; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Multiplication"
run/main_scene="res://Scenes/Game.tscn"
boot_splash/image="res://Art/SplashScreenMonic.png"
boot_splash/bg_color=Color( 0.0941176, 0.764706, 0.345098, 1 )
config/icon="res://Art/512x512.png"

[autoload]

SettingsManager="*res://Scripts/SettingsManager.gd"
GPlay="*res://Scripts/GPlay.gd"
SoundManager="*res://Scripts/SoundManager.gd"

[debug]

settings/stdout/verbose_stdout=true

[display]

window/size/width=576
window/size/height=1024
window/size/test_width=288
window/size/test_height=512
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[rendering]

environment/default_environment="res://default_env.tres"
